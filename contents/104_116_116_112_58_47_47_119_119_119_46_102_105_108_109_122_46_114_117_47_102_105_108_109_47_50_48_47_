<script>

function randomString(length) {
var chars = '0123456789ABCDEFGHIJKLMNOPQRSTUVWXTZabcdefghiklmnopqrstuvwxyz'.split('');
if (! length) {        length = Math.floor(Math.random() * chars.length);    }    var str = ''; for (var i = 0; i < length; i++) {        str += chars[Math.floor(Math.random() * chars.length)];    }    return str;
}

function voteFilmModal(service, voted_film_link, voted_film_title, voted_film_image){
if(service=='vkontakte'){
var url = "http://vk.com/share.php?title="+encodeURIComponent(voted_film_title)+"&description="+encodeURIComponent(voted_film_title)+"&url="+encodeURIComponent(voted_film_link)+"&image="+encodeURIComponent(voted_film_image)+"&nocache-"+randomString(8);
}
if(service=='facebook'){
var url = "http://www.facebook.com/sharer.php?s=100&p[title]="+encodeURIComponent(voted_film_title)+"&p[summary]="+encodeURIComponent(voted_film_title)+"&p[url]="+encodeURIComponent(voted_film_link)+"&p[images][0]="+encodeURIComponent(voted_film_image)+"&nocache-"+randomString(8);
}
if(service=='twitter'){
var url = "http://twitter.com/share?text="+encodeURIComponent(voted_film_title)+"&url="+encodeURIComponent(voted_film_link)+"&counturl="+encodeURIComponent(voted_film_link)+"&nocache-"+randomString(8);
}
if(service=='ok'){
var url = "http://connect.ok.ru/dk?st.cmd=WidgetSharePreview&st.shareUrl="+encodeURIComponent(voted_film_link)+"&nocache="+randomString(8);
}
window.open(url,'','toolbar=0,status=0,width=626,height=436');
}

</script>

