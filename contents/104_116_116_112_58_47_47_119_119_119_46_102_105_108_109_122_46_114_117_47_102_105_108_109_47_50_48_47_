<script type="text/javascript">
	var current_mark = Math.round(0);
	voted=(current_mark>0)?true:false;

	function film_mark_onmove(mark)
	{
		for ( var i = 1; i <= mark; i++ )
		{
			document.getElementById("icon16_" + i).src = "/images/icons/starsrate32-b.png";
		}

		for ( var i = mark + 1; i <= 5; i++ )
		{
			document.getElementById("icon16_" + i).src = "/images/icons/starsrate32-g.png";
		}
	}
	
	function film_mark(vote)
	{
		current_mark=vote;
		film_mark_onout();
		$.get('/films/vote.php?id=20&vote='+vote, function(data){
		if(voted==false){
		document.getElementById('siterate_count_value').innerHTML=document.getElementById('siterate_count_value').innerHTML*1+1;
		voted=true;
		}
		$('#film-after-vote-window').modal({});
		voted_film_title='Я оцениваю фильм "Анализируй это" на '+vote+' из 5';
		voted_film_link='http://www.filmz.ru/film/20/';
		voted_film_image='http://media.filmz.ru/photos/small/filmz.ru_s_32728.jpg';
		});
	}

	function film_mark_onout()
	{
		film_mark_onmove(current_mark);
	}

</script>



