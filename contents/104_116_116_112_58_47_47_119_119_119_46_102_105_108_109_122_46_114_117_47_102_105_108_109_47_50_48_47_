<script>
function deleteMyFilmNote(id){
ajaxPOSTRequest('/films/delete_note.php', 'id='+id, responseDeleteMyFilmNote);
}

function responseDeleteMyFilmNote(result){
	$('#note'+result).remove();
}
</script>
	

	
	

