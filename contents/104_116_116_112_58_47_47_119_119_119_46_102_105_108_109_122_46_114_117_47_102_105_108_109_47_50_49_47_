<script language=JavaScript>

var  comments = new Array(); 

var current_reply_form_id=0;
var current_textarea_id=0;

var  comments_service = new Array(); 
comments_service["count_comments"] = "0"; 
comments_service["count_pages"] = "0"; 
comments_service["current_page"] = "1"; 
comments_service["category"] = "films"; 
comments_service["article_id"] = 21; 
comments_service["comment_id"] = "0"; 
comments_service["user_login"] = 0; 
comments_service["error"] = "error_none";

function postFilmOpinion(){

document.getElementById("comment_reply_form_0").disabled = true;
//document.getElementById("comment_reply_form_span_loading_0").style.display = "inline";

ajaxPOSTRequest(comments_settings["ajax_handler_path"] + "?action=add",$('#comment_reply_form_0').serialize(),responsepostFilmOpinion);

}


function responsepostFilmOpinion(result){

if(comments_service["error"] == "error_none" && (document.getElementById('comment_reply_form_0_input_private').checked || document.getElementById('comment_reply_form_0_input_review').checked)){

window.location.reload();

} else {

var  comments = new Array(); 

eval(result);

if (comments_service["error"] == "error_none")document.location.replace("/film/" + comments_service["article_id"] + "/comments/?my_last_comment_id=1#comments");
else alert("Возникла ошибка при отправке мнения: " + comments_service["error"]);

document.getElementById("comment_reply_form_0").disabled = false;
document.getElementById("comment_reply_form_span_loading_0").style.display = "none";

}

}

</script>

<script>
function check_is_review(){
var val=document.getElementById('comment_reply_form_textarea_0').value;
var length=val.length;
if(length>=1000){
document.getElementById('is_review').style.display='inline-block';
} else {
document.getElementById('is_review').style.display='none';
}
}
</script>

